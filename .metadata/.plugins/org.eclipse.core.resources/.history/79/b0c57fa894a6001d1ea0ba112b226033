package array;import java.util.Random;

public class Lotto {

	public static void main(String[] args) {
		int[] lotto;
		lotto = new int[6];
		
		for(int i = 0; i < lotto.length - 1; i++) {
			lotto[i] = (int)(Math.random() * 45);
			System.out.println(lotto[i]);
		}
		

	}

}

