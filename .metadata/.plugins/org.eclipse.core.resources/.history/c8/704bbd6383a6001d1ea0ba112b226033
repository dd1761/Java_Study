package multi_;

public class MultiFor04 {

	public static void main(String[] args)
	{
		for(int i = 2; i <= 9; i=i+3) {
			for(int j = 1; j <= 9; j++) {
				for(int dan = i; dan < i + 3; dan++) {
					if(dan == 10) {
						break;
					}
					System.out.print(dan + " * " + j + " = " + dan * j + "\t");
					
				}
				System.out.println();
			}
			System.out.println();
		}
	}
}


// 2 ~ 4(for문 처음 반복) 
// 5 ~ 7(for문 두번 반복)
// 8 ~ 10(for문 마지막 반복)