package multi_;

public class MultiFor04 {

	public static void main(String[] args)
	{
		for(int i = 2; i < 10; i = i + 2) {
			for(int j = 1; j <= 9; j++) {
				for(int dan = i; dan <= i + 3; dan++) {
					System.out.print(dan + " * " + j + " = " + dan * j + "\t");
					if(dan >= 10) {
						break;
					}
				}System.out.println();
			}System.out.println();
		}
	}
}
