package array;

public class BubbleSort {

	public static void main(String[] args) {
		int[] ar = { 32, 40, 25, 78, 56 };

		// 출력
		

		// 정렬
		// 오름차순(ASCENDING)
		// 내림차순(DECENDING)
		int temp;
		for (int i) {
			for (int j) {
				if(ar[] < ar[]) {		// > 오름차순, < 내림차순
					temp = ar[];
					ar[] = ar[];
					ar[] = temp;
					
				}
			}
		}
		for (int i = 0; i < ar.length; i++) {
			System.out.print(String.format("%4d", ar[i]));
		}
		System.out.println();

	}

}
