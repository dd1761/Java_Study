package array;

public class SelectionSort {

	public static void main(String[] args) {
		int[] ar = { 25, 40, 32, 78, 56 };
		
		//출력
		for(int i = 0; i < ar.length; i++) {
			System.out.print(String.format("%4dx", ar[i]));
		}
		System.out.println();
		
		//정렬
		// 오름차순(ASCENDING)
		// 내림차순(DECENDING)
		
	}

}
